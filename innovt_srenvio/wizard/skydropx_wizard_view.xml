<odoo>
    <data>
        <record id="msb_skydroox_provider_wizard_form" model="ir.ui.view">
                <field name="name">skydropx.provider.wizard.form</field>
                <field name="model">skydropx.provider.wizard</field>
                <field name="arch" type="xml">
                <form>             
                    <group>
                        <field class="oe_inline" name="provider"  widget="radio" required="True"/>
                    </group>
                    <footer>
                        <button name="ok" string="Aceptar" type="object" default_focus="1" class="oe_highlight"/>
                        <button string="Cancelar" class="btn btn-secondary" special="cancel" />
                    </footer>
                </form>
                </field>
            </record>
    </data>
    
</odoo>